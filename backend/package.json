{
  "name": "ecomestivel-backend",
  "version": "1.0.0",
  "description": "Backend para aplicação web de catalogação de plantas urbanas",
  "main": "dist/server.js",
  "scripts": {
    "prisma:seed": "dotenv -e .env -- ts-node prisma/seed.ts",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:studio": "dotenv -e .env -- prisma studio",
    "dev": "dotenv -e .env -- nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "dotenv -e .env -- node dist/server.js",
    "test:db": "dotenv -e .env -- ts-node src/test/db.test.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "plantas",
    "urbana",
    "comestivel",
    "catalogo",
    "colaborativo"
  ],
  "author": "É de Comer?",
  "license": "MIT"
}
